project_text=CMIP5
esmmod=MPI-ESM
expName=$(echo $expid   | cut -f1 -d"-")
member=$(echo  $expName | cut -f2 -d"_")        
expName=$(echo $expName | cut -f1 -d"_")
[[ $expName = abrupt4xco2 ]] && expName=abrupt4xCO2
[[ $expName = 1pctCo2 ]] && expName=1pctCO2
res_mod=$(echo $expid   | cut -f2 -d"-")
